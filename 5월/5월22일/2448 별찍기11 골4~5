def merge(l1,l2,l3):
    return zip(l1,l2,l3,l2,l1)
    #l=[''.join(i) for i in list(zip(l1,l2,l3,l2,l1))]
    #return l
def star(x):
    x//=3
    if x==0:
        return '*'
    s=star(x)
    t=merge([' '*x],[' '*x],[s])
    m=merge([' '*x],[s],[' '*x])
    b=merge([s],[s],[s])
    return t + m + b

a=star(3**int(input()))
print(a)
